{
    "keymaps": {
        "GLOBAL": {
            "Alt+C": "rzm.toggle_debug",
            "Ctrl+Z": "rzm.undo",
            "Ctrl+Shift+Z": "rzm.redo",
            "F5": "rzm.refresh"
        },
        "VIEWPORT": {
            "H": "rzm.toggle_hide",
            "Alt+H": "rzm.unhide_all",
            "L": {
                "op": "rzm.toggle_lock",
                "args": {}
            },
            "Left": {
                "op": "rzm.viewport_arrow",
                "args": {
                    "x": -10,
                    "y": 0
                }
            },
            "Right": {
                "op": "rzm.viewport_arrow",
                "args": {
                    "x": 10,
                    "y": 0
                }
            },
            "Up": {
                "op": "rzm.viewport_arrow",
                "args": {
                    "x": 0,
                    "y": -10
                }
            },
            "Down": {
                "op": "rzm.viewport_arrow",
                "args": {
                    "x": 0,
                    "y": 10
                }
            },
            "Shift+Left": {
                "op": "rzm.nudge",
                "args": {
                    "x": -1,
                    "y": 0
                }
            },
            "Shift+Right": {
                "op": "rzm.nudge",
                "args": {
                    "x": 1,
                    "y": 0
                }
            },
            "Shift+Up": {
                "op": "rzm.nudge",
                "args": {
                    "x": 0,
                    "y": -1
                }
            },
            "Shift+Down": {
                "op": "rzm.nudge",
                "args": {
                    "x": 0,
                    "y": 1
                }
            },
            "Delete": "rzm.delete",
            "Ctrl+A": "rzm.select_all",
            "Home": "rzm.view_reset"
        },
        "OUTLINER": {
            "H": "rzm.toggle_hide",
            "Alt+H": "rzm.unhide_all",
            "Delete": "rzm.delete",
            "Ctrl+V": "rzm.select_all"
        },
        "INSPECTOR": {}
    },
    "appearance": {
        "theme": "light",
        "custom_bg_path": ""
    }
}