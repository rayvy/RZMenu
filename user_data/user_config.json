{
    "keymaps": {
        "GLOBAL": {
            "Ctrl+Z": "rzm.undo",
            "Ctrl+Shift+Z": "rzm.redo",
            "F5": "rzm.refresh"
        },
        "VIEWPORT": {
            "Delete": "rzm.delete",
            "Ctrl+A": "rzm.select_all",
            "Home": "rzm.view_reset",
            "Left": {
                "op": "rzm.viewport_arrow",
                "args": {
                    "x": -10,
                    "y": 0
                }
            },
            "Right": {
                "op": "rzm.viewport_arrow",
                "args": {
                    "x": 10,
                    "y": 0
                }
            },
            "Up": {
                "op": "rzm.viewport_arrow",
                "args": {
                    "x": 0,
                    "y": -10
                }
            },
            "Down": {
                "op": "rzm.viewport_arrow",
                "args": {
                    "x": 0,
                    "y": 10
                }
            }
        },
        "OUTLINER": {
            "Delete": "rzm.delete",
            "Ctrl+A": "rzm.select_all"
        },
        "INSPECTOR": {}
    }
}